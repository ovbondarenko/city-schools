    "# Assign the following variables:\n",
    "\n",
    "spending_bins = [0, 585, 615, 645, 675]\n",
    "group_names = [\"<$585\", \"$585-615\", \"$615-645\", \"$645-675\"]\n",
    "newtitle = 'Spending Ranges (per Student)' #The new title of the bins for a summary dataframe\n",
    "binby = 'Per Student Budget' # Column in the school_summary dataframe that will be binned as specified \n",
    "\n",
    "def binned(df, groups, group_names, binby, newtitle):\n",
    "    \n",
    "    # Create a new column in the original dataframe containing the group name of the record\n",
    "    df[newtitle] = pd.cut(df[binby], bins = groups, labels = group_names).to_frame()\n",
    "    \n",
    "    # Group by this new column\n",
    "    grouped = df.groupby(newtitle)\n",
    "    new_df = grouped['Average Math Score'].mean().to_frame()\n",
    "    new_df['Average Reading Score'] = grouped['Average Reading Score'].mean()\n",
    "    new_df['% Passing Math'] = grouped['% Passing Math'].mean()\n",
    "    new_df['% Passing Reading'] = grouped['% Passing Reading'].mean()\n",
    "    new_df['% Overall Passing Rate'] = grouped['% Overall Passing Rate'].mean()\n",
    "\n",
    "    return formatter(new_df, mapper) # Format the dataframe\n",
    "\n",
    "\n",
    "binned(grouped_byname(), spending_bins, group_names, binby, newtitle)"
   ]
