   "source": [
    "# Formatting the summary dataframe    \n",
    "\n",
    "# specify a formatting method for each dataframe column to be formatted\n",
    "mapper = {'Total Budget': \"${:,}\",\n",
    "          'Per Student Budget': \"${0:g}\",\n",
    "          'Total School Budget': \"${:,}\",\n",
    "          'Total Students': \"{:,}\",\n",
    "          '% Passing Math': \"{:.2f}%\",\n",
    "          '% Passing Reading': \"{:.2f}%\",\n",
    "          '% Overall Passing Rate': \"{:.2f}%\",\n",
    "          'Average Reading Score': \"{:.2f}\",\n",
    "          'Average Math Score': \"{:.2f}\"\n",
    "         }\n",
    "\n",
    "def formatter(df, mapper):\n",
    "    ''' \n",
    "    formatter(mapper, df) function accepts two variables:\n",
    "    - dataframe to be formatted\n",
    "    - dictionary of column names (keys) and their corresponding formatting method strings (values)\n",
    "    \n",
    "    formatter(mapper, df) returns a formatted dataframe\n",
    "    '''\n",
    "    for key, value in mapper.items():\n",
    "        if key in list(df):\n",
    "            df[key] = df[key].apply(value.format)\n",
    "    return df\n",
    "\n",
    "# Format the district summary table\n",
    "formatter(district_summary, mapper)"
   ]
